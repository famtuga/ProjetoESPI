@model Horarios.Models.EstudanteDisciplina

@{
    ViewData["Title"] = "Edit";
}

<h2>Editar</h2>

<h4>EstudanteDisciplina</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EstudanteId" />
            <input type="hidden" asp-for="DisciplinaId" />
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar ao menu anterior</a>
</div>

